sim:/unsigned_seq_multiplier/a \
sim:/unsigned_seq_multiplier/b \
sim:/unsigned_seq_multiplier/c \
sim:/unsigned_seq_multiplier/clk \
sim:/unsigned_seq_multiplier/rst
